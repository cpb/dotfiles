[user]
	name = Jeff Wallace
	email = <%= print("Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[github]
	user = tjwallace
	token = <%= print("Your github token: "); STDOUT.flush; STDIN.gets.chomp %>
[alias]
	ci = commit -a
	co = checkout
	st = status
	up = pull --rebase
	hist = log --pretty=format:\"%h | %ad | %s%Cred%d%Creset %Cgreen[%an]%Creset\" --graph --date=short
	d = diff --word-diff
[color]
	branch = auto
	diff = auto
	status = auto
	ui = auto
	grep = auto
	interactive = auto
	pager = true
[core]
	editor = /usr/bin/vim
	pager = most
	excludesfile = ~/.gitignore
[push]
	default = matching
[branch]
	autosetuprebase = always
