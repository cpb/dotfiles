[user]
	name = <%= print("Your Name: "); STDOUT.flush; STDIN.gets.chomp %> 
	email = <%= print("Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[github]
	user = tjwallace
[alias]
	ci = commit -a
	co = checkout
	st = status
	up = pull --rebase
	d = diff --word-diff
[color]
	branch = auto
	diff = auto
	status = auto
	ui = auto
	grep = auto
	interactive = auto
	pager = true
[core]
	editor = /usr/bin/vim
	pager = most
	excludesfile = ~/.gitignore
[push]
	default = matching
[branch]
	autosetuprebase = always
